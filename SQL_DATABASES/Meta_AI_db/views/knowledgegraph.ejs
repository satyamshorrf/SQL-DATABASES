<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table, 
        th, 
        td 
        {
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>KnowledgeID</th>
            <th>Topic</th>
            <th>Description</th>
            <th>EntityID</th>
        </tr>
        <% for(user of users) { %>
            <tr>
                <td><%= user.KnowledgeID %></td>
                <td><%= user.Topic %></td>
                <td><%= user.Description %></td>
                <td><%= user.EntityID %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>