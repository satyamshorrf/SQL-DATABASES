<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table, 
        th, 
        td 
        {
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
  <th>UserID </th>
  <th>Username </th>
  <th>ConversationID </th>
  <th>StartDate </th>
  <th>EndDate </th>
  <th>MessageID </th>
  <th>MessageText </th>
  <th>Timestamp </th>
  <th>IntentName </th>
  <th>EntityName </th>
  <th>EntityType </th>
  <th>Topic </th>
  <th>Description </th>
  <th>ResponseText </th>
  <th>Timestamp </th>
  <th>Rating </th>
  <th>Comment </th>
        </tr>
        <% for(user of users) { %>
            <tr>
                <td><%= user.UserID %></td>
                <td><%= user.Username %></td>
                <td><%= user.ConversationID %></td>
                <td><%= user.StartDate %></td>
                <td><%= user.EndDate %></td>
                <td><%= user.MessageID/td>
                <td><%= user.Timestamp %></td>
                <td><%= user.IntentName %></td>
                <td><%= user.EntityName %></td>
                <td><%= user.EntityType %></td>
                <td><%= user.Topic %></td>
                <td><%= user.Description %></td>
                <td><%= user.ResponseText %></td>
                <td><%= user.Timestamp %></td>
                <td><%= user.Rating %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>